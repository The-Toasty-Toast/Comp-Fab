 import Turtle.*;

Turtle t; 

void setup() {
  size(800, 800);
  background(0);
  stroke(255);
  strokeWeight(2);
  t = new Turtle(this);
  //noLoop();
// very similar to the code that I wrote for I but I make the main body of the T shorter
    t.right(90);
    t.forward(50);
    t.penUp();
    t.back(25);
    t.right(90);
    t.penDown();
    t.forward(75);
}
